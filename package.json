{
  "name": "fido-sba-copilot",
  "version": "1.0.0",
  "description": "AI-powered SBA loan guidance chatbot with business plan builder",
  "main": "backend/index.js",
  "scripts": {
    "start": "node backend/index.js",
    "dev": "nodemon backend/index.js",
    "frontend": "cd frontend && python3 -m http.server 5500",
    "dev:full": "concurrently \"npm run dev\" \"npm run frontend\"",
    "install:all": "npm install && cd backend && npm install",
    "test": "echo \"No tests specified\" && exit 0",
    "build": "echo \"No build step required\"",
    "deploy:check": "node scripts/deploy-check.js",
    "health": "curl http://localhost:3000/health",
    "docker:build": "docker build -t fido-sba-copilot .",
    "docker:run": "docker run -p 80:80 -p 3000:3000 fido-sba-copilot",
    "docker:compose": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "setup:env": "cp backend/.env.example backend/.env",
    "setup:github": "bash scripts/setup-github.sh",
    "deploy:render": "echo 'Deploy to Render via GitHub Actions or manual deployment'",
    "deploy:vercel": "echo 'Deploy to Vercel via GitHub Actions or Vercel CLI'",
    "deploy:railway": "echo 'Deploy to Railway via GitHub Actions or Railway CLI'"
  },
  "keywords": [
    "sba",
    "loan",
    "chatbot",
    "ai",
    "business-plan",
    "entrepreneur",
    "fintech"
  ],
  "author": "Fido Team",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.0",
    "concurrently": "^8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/fido-sba-copilot.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/fido-sba-copilot/issues"
  },
  "homepage": "https://github.com/yourusername/fido-sba-copilot#readme"
} 